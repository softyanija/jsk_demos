(defun voice_confirm()
  (while (ros::ok)
    (setq message (one-shot-subscribe "/Tablet/voice" speech_recognition_msgs::SpeechRecognitionCandidates))    (setq message (elt (send message :transcript) 0))
    (print message)
    (if (or (string= message "終わりました") (string= message "終わった")) (return)
      )
    )
  )
